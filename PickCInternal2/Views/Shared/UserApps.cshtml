@model  Operation.Contract.UserData
            <div class="tab-main-content">

                <div class="row">
                    <div class="col-md-3 col-sm-6 col-xs-12" id="AppDownloadsBox" onclick="getDownloadList();">
                        
                            <span class="info-box-icon bg-aqua"><i class="fa fa-download" aria-hidden="true"></i></span>
                        <div class="info-box">
                            <div class="info-box-content" >
                                <span class="info-box-text">User App Downloads</span>
                                <span class="info-box-number">IOS: <span>200</span></span></span>
                                <span class="info-box-number"> Android: <span>300</span></span><br />
                                
                            </div>
                            <span class="info-box-number total-box" style="margin-top:-10px;"><b>Total:</b> <span>1000</span></span>
                            <!-- /.info-box-content -->
                        </div>
                       
                        <!-- /.info-box -->
                    </div>
                    
                    <div class="col-md-3 col-sm-6 col-xs-12 hand" id="RegisteredBox" onclick="getRegisteredList();">
                        
                            <span class="info-box-icon bg-green"><i class="fa fa-registered" aria-hidden="true"></i></span>
                        <div class="info-box">
                            <div class="info-box-content">
                                <span class="info-box-text">Registered But Not Booked </span>
                                <span class="info-box-number">Today Booked:<span>@Model.userDataDashBoard.RegisteredNotBookDailyBasis</span></span>
                                <br/>
                                @*<span class="info-box-text">Total Count</span>*@
                                
                            </div>
                            <span class="info-box-number total-box" style="background:#00a65a;"><span class="total-text">Total:</span><span>@Model.userDataDashBoard.RegisteredNotBook</span></span>
                            @*<div class="info-box-content">
                <span class="info-box-text">Registered But Not Booked</span>
                <span class="info-box-number">@Model.userDataDashBoard.RegisteredNotBook</span>
            </div>*@
                            <!-- /.info-box-content -->
                        </div>
                        <!-- /.info-box -->
                    </div>
                    <div class="col-md-3 col-sm-6 col-xs-12 hand" id="totaltripsBox" onclick="getTotalTrips();">
                        
                            <span class="info-box-icon bg-red"><i class="fa fa-tripadvisor" aria-hidden="true"></i></span>
                        <div class="info-box">
                            <div class="info-box-content">
                                <span class="info-box-text">Total Trips </span>
                                <span class="info-box-number">Today Trips: <span>@Model.userDataDashBoard.TotalBookingsdailyBasis</span></span><br/>
                                
                                @*<span class="info-box-number">@Model.userDataDashBoard.TotalBookings</span>*@
                            </div>
                            <span class="info-box-number total-box" style="background:#cccccc; margin-top:15px;"><span class="total-text">Total:</span><span>@Model.userDataDashBoard.TotalBookings</span></span>
                            <!-- /.info-box-content -->
                        </div>
                        <!-- /.info-box -->
                    </div>
                    <div class="col-md-3 col-sm-6 col-xs-12 hand" id="CancellationBox" onclick="getCancelledList();">
                        
                            <span class="info-box-icon bg-yellow"><i class="ion ion-ios-people-outline"></i></span>
                        <div class="info-box">

                            <div class="info-box-content">
                                <span class="info-box-text">Cancellations</span>
                                <span class="info-box-number">Driver: <span>@Model.userDataDashBoard.DriverCancel</span></span>
                                <span class="info-box-number"> Customer: <span>@Model.userDataDashBoard.CustomerCancel</span> </span>
                                @*@{ var data =(Operation.Contract.UserDataDashBoard)TempData["count"];

                                 }*@
                            </div>
                            <span class="info-box-number total-box" style="background:#f39c12;"> <span class="total-text">Total:</span> <span>@Model.userDataDashBoard.TotalCancellations</span></span>
                            <!-- /.info-box-content -->
                        </div>
                        <!-- /.info-box -->
                    </div>
                </div>
            </div>

<style>
    .total-box{
    margin-top: 10px;
    color: #fff;
    padding: 5px 0px 5px 25px;
    background: #8bdbef;
    }
    .info-box-number > span{
        float:right;
        margin-right:10px;
        font-weight:bold;
    }
  .info-box-number > p{
      background:#fff;
  }
  .total-text{
      float:none !important;
          background: #fff;
    color: #000;
    padding: 0.35em 1.28em;
    position: relative;
    left: -25px;
    font-weight:normal !important;

  }
</style>
<script src="@Url.Content("~/Scripts/jquery.validate.js")"></script>

    <script type="text/javascript">
        $(function () {
            $('#userApp').parent().addClass('active');
                                });
                                function getTotalTrips() {
                                    window.location = "@Url.Content("~/Dashboard/UserApp")";
                                }
                                function getRegisteredList() {
                                    window.location = "@Url.Content("~/Dashboard/RegisteredList")";
                                }
                                function getCancelledList() {
                                    window.location = "@Url.Content("~/Dashboard/UserAppCancellation")";
                                }


    </script>





